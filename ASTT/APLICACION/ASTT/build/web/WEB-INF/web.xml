<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>UsuarioServlet</servlet-name>
        <servlet-class>astt.una.prograiv.sistema.controller.UsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChoferServlet</servlet-name>
        <servlet-class>astt.una.prograiv.sistema.controller.ChoferServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChofervehiculoServlet</servlet-name>
        <servlet-class>astt.una.prograiv.sistema.controller.ChofervehiculoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServicioServlet</servlet-name>
        <servlet-class>astt.una.prograiv.sistema.controller.ServicioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsuarioServlet</servlet-name>
        <url-pattern>/UsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>VehiculoServlet</servlet-name>
        <servlet-class>astt.una.prograiv.sistema.controller.VehiculoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>astt.una.prograiv.sistema.controller.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VehiculoServlet</servlet-name>
        <url-pattern>/VehiculoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChoferServlet</servlet-name>
        <url-pattern>/ChoferServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChofervehiculoServlet</servlet-name>
        <url-pattern>/ChofervehiculoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServicioServlet</servlet-name>
        <url-pattern>/ServicioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
